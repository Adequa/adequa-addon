{
  "author": "Adequa",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icon_dev_16.png",
      "32": "img/icon_dev_32.png"
    },
    "default_title": "Adequa dev",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "/js/page/messaging.js",
        "/js/page/contentscript.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/js/page/notify.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://*.leboncoin.fr/*"
      ],
      "js": [
        "/js/page/leboncoin.js"
      ],
      "all_frames": true,
      "run_at": "document_idle"
    }
  ],
  "default_locale": "fr",
  "description": "__MSG_extShortDesc__",
  "icons": {
    "16": "img/icon_dev_16.png",
    "32": "img/icon_dev_32.png",
    "64": "img/icon_dev_64.png",
    "128": "img/icon_dev_128.png"
  },
  "incognito": "spanning",
  "manifest_version": 2,
  "minimum_chrome_version": "51.0",
  "web_accessible_resources": [
    "/js/page/cmp.js",
    "/js/page/adequa-cmp.js",
    "/js/page/start-cmp.js",
    "/js/page/adequa-api.bundle.js"
  ],
  "name": "Adequa dev",
  "options_page": "dashboard.html",
  "permissions": [
    "contextMenus",
    "storage",
    "tabs",
    "cookies",
    "unlimitedStorage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "short_name": "Adequa dev",
  "version": "1.0.57"
}
